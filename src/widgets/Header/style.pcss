.header {
  color: var(--colorWhite);
  background-color: var(--colorShark);
  padding: 16px 0;
  min-height: 64px;

  &__inner {
    display: flex;
    justify-content: space-between;
    align-items: center;
  }

  &__title {
    font-size: 20px;
    line-height: 1.6;
    font-weight: 600;
    flex-shrink: 0;
  }
}
